{
  "name": "complex",
  "version": "1.0.0",
  "author": "Peter Lager",
  "license": "MIT",
  "description": "A complex number class including many trig functions.",
  "main": "complex.js",
  "browser": "complex.js",
  "module": "complex.mjs",
  "types": "typings/complex.d.mts",
  "keywords": [
    "complex", "cmath", "trig", "trigonometry",
    "jsdocs", "tsdocs", "jest", "tsd",
    "hashcode", "equality", "epsilon",
    "iterator", "comparator",
    "this", "polymorphic", "polymorphism", "subclasses",
    "esmodules", "esm", "mjs", "mts"
  ],
  "scripts": {
    "preinstall": "cd scripts && dev-packs",
    "test": "cd scripts && test",
    "start": "cd scripts && run-docs",
    "server": "cd scripts && run",
    "compile": "tsc",
    "check": "tsd -f tests/complex.test-d.mts",
    "checks": "run-p -cl compile test check",
    "tsdoc": "typedoc",
    "jsdoc": "jsdoc -c jsdoc.json",
    "docs": "run-p -cl tsdoc jsdoc",
    "all": "npm-run-all -cl test compile check -p tsdoc jsdoc"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
  },
  "peerDependencies": {
    "cross-env": "^7",
    "npm-run-all": "^4",
    "servor": "^4",
    "typescript": "^5",
    "jsdoc": "^4",
    "typedoc": "^1",
    "jest": "^29",
    "@jest/globals": "^29",
    "tsd": "x"
  },
  "peerDependenciesMeta": {
    "cross-env": {
      "optional": true
    },
    "npm-run-all": {
      "optional": true
    },
    "servor": {
      "optional": true
    },
    "typescript": {
      "optional": true
    },
    "jsdoc": {
      "optional": true
    },
    "typedoc": {
      "optional": true
    },
    "jest": {
      "optional": true
    },
    "@jest/globals": {
      "optional": true
    },
    "tsd": {
      "optional": true
    }
  },
  "jest": {
    "testMatch": ["**/tests/**/*.jest.mjs"],
    "transform": {},
    "collectCoverage": true
  }
}
